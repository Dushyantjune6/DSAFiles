import java.util.*;
public class Main
{
    public static int findOddInArrayOfDuplicates(int[] arr){
         int unique = 0;
         for(int n: arr){
             unique ^=n;
         }
         return unique;
    }
    public static void main(String[] args){
        int[] arr = {2,3,3,3,3,3,3,3,4,2,4};
        System.out.println(findOddInArrayOfDuplicates(arr));
    }
}
